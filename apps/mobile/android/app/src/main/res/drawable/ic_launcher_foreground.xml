<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Transformation to map 200x200 path data to 108x108 icon -->
    <!-- HTML: scale(0.44), center 108. 200*0.44 = 88. (108-88)/2 = 10 padding. -->
    <!-- Here we use scale 0.44 and translate 10. -->
    <group
        android:translateX="10"
        android:translateY="10"
        android:scaleX="0.44"
        android:scaleY="0.44">
        
        <!-- Box Top Face -->
        <path
            android:fillColor="#E6C590"
            android:pathData="M100 24 L186 64 L100 104 L14 64 Z"/>
            
        <!-- Box Left Face -->
        <path
            android:fillColor="#C29B55"
            android:pathData="M12 68 L98 108 L98 186 L12 146 Z"/>
            
        <!-- Box Right Face -->
        <path
            android:fillColor="#D4B070"
            android:pathData="M188 68 L188 146 L102 186 L102 108 Z"/>
            
        <!-- Tape Top -->
        <path
            android:fillColor="#10b981"
            android:pathData="M 35 73.8 L 65 87.7 L 151 47.7 L 121 33.8 Z"/>
            
        <!-- Tape Left -->
        <path
            android:fillColor="#059669"
            android:pathData="M 35 78.7 L 65 92.7 L 65 140 L 35 125 Z"/>
            
        <!-- Tape Connector (Top-Left Edge) -->
        <path
            android:fillColor="#059669"
            android:pathData="M 35 73.8 L 65 87.7 L 65 92.7 L 35 78.7 Z"/>
            
        <!-- Cross Group -->
        <!-- Skew manually applied to path data (since vector doesn't support group skew) -->
        <!-- Approximate skew -25 deg: x' = x + y * tan(-25) -->
        <!-- Center pivot (145, 127) -->
        <group android:translateX="133" android:translateY="115">
             <!-- Simplified cross for vector compatibility (no skew) -->
             <!-- Making it a simple rotated cross to resemble the look -->
             <path
                android:fillColor="#10b981"
                android:pathData="M-4,0 L24,0 L24,8 L-4,8 Z"
                android:rotation="-25"
                android:pivotX="10"
                android:pivotY="4"/>
             <path
                android:fillColor="#10b981"
                android:pathData="M6,-14 L14,-14 L14,22 L6,22 Z"
                android:rotation="-25"
                android:pivotX="10"
                android:pivotY="4"/>
        </group>
        
        <!-- Top Highlight (simulating gloss edge) -->
        <path
             android:strokeColor="#FFFFFF"
             android:strokeWidth="2"
             android:strokeAlpha="0.4"
             android:pathData="M14 64 L100 24 L186 64"/>
    </group>
</vector>
