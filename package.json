{
  "name": "appteczka-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm -w @appteczka/web run dev",
    "build:web": "npm -w @appteczka/web run build",
    "start:web": "npm -w @appteczka/web run start",
    "lint:web": "npm -w @appteczka/web run lint",
    "vercel-build": "cd apps/web && npm run build"
  }
}